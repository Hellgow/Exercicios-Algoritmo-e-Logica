algoritmo "Exercicio67"
var
   num,cont:inteiro

inicio

      escreval("------------ Repetição com PARA ------------")
      escreval(" ")
      
      escreva("Digite um numero inteiro: ")
      leia(num)
      escreval (" ")
      
      para cont <- 0 ate num passo 1 faca
           escreva(cont,",")
      fimpara
      
      escreval (" terminei a contagem.")
      

fimalgoritmo