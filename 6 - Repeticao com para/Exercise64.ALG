algoritmo "Exercicio64"
var
   cont:inteiro

inicio
      escreval ("---------- Repetição com PARA ----------")
      escreval (" ")
      
      para cont <- 0 ate 40 passo 5 faca
           escreva(cont)
      fimpara
      
      escreva(" Terminei de contar.")
      escreval(" ")
      

fimalgoritmo