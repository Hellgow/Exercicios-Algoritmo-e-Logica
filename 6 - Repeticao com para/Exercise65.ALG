algoritmo "Exercicio65"
var
   cont:inteiro

inicio

      escreval("---------- Repetição com PARA -----------")
      escreval(" ")
      
      para cont <-  100 ate 0 passo -10 faca
           escreva(cont)
      fimpara
      
      escreva(" Terminei de contar.")
      escreval(" ")

fimalgoritmo