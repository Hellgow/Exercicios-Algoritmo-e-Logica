algoritmo "Exercicio7"
var
   N1, D, T: Real
   

inicio
      Escreva ("Por favor informe um numero aleatorio: ")
      Leia (N1)
      Escreval (" ")
      
      D <- N1 * 2
      T <- N1 / 3
      
      Escreva ("O dobro de", N1, " é igual a", D)
      Escreval (" ")
      Escreva ("A terça parte de", N1, " é igual a", T)
      Escreval (" ")

fimalgoritmo