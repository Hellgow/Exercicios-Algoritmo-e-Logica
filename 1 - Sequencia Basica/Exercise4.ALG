algoritmo "Exercicio4"
var
   N1, N2, S1: Inteiro

inicio
      Escreva ("Informe o primeiro numero aleatorio: ")
      Leia (N1)
      
      Escreva ("Informe o segundo numero aleatorio: ")
      Leia (N2)
      Escreval (" ")
      
      S1 <- N1 + N2
      
      Escreva ("A soma entre ", N1, " e", N2, " é igual a", S1)
      Escreval (" ")

fimalgoritmo