algoritmo "Exercicio5"
var
   N1, N2, MD: Real

inicio
      Escreva ("Por favor informe a nota da primeira prova: ")
      Leia (N1)
      
      Escreva ("Por favor informe a nota da segunda prova: ")
      Leia (N2)
      Escreval (" ")
      
      MD <- (N1 + N2) / 2
      
      Escreva ("A media entre ", N1, " e", N2, " é igual a", MD)
      Escreval (" ")

fimalgoritmo