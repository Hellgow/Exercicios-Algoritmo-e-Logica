algoritmo "Exercicio13"
var
   Salario, Aumento, SalarioFinal: Real

inicio
      Escreva ("Por favor, informe seu salario atual R$ ")
      Leia (Salario)
      Escreval (" ")
      
      Aumento <- (Salario*15) /100
      SalarioFinal <- Salario + Aumento
      
      Escreva ("Seu salario com 15% de aumento fica em R$", SalarioFinal)
      Escreval (" ")
      

fimalgoritmo