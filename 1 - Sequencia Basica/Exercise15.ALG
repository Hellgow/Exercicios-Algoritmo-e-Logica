algoritmo "Exercicio15"
var
   DiasT, Salario, Salariomes: Real

inicio
      Escreva ("Quantos dias você trabalhou este mês? ")
      Leia (DiasT)
      Escreval (" ")
      
      Salario <- 25*8
      Salariomes <- Salario * DiasT
      
      Escreva ("Seu salario com base em", DiasT, " dia trabalhado, é de R$", Salariomes)
      Escreval (" ")
      
      

fimalgoritmo