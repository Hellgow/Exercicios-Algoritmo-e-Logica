algoritmo "Exercicio3"
var
   Nome:Caractere
   Salario:Real

inicio
   Escreva ("Por favor, informe seu nome completo: ")
   Leia (Nome)
   Escreval (" ")

   Salario <- 1850.45

   Escreva ("O funcionario(a) ", Nome, " recebeu o valor de R$", Salario, " no mes de Junho.")
   Escreval (" ")

fimalgoritmo