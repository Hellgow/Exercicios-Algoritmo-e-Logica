algoritmo "Exercicio61"
var
   cont:inteiro
   
inicio
       escreval("------------ Repetição com Faça - Enquanto -------------")
       escreval (" ")

      cont <- 0
      enquanto (cont <= 30) faca
               escreva (cont)
               cont <- cont + 3
      
      fimenquanto
      escreval(" Terminei de contar!")

fimalgoritmo